@model IEnumerable<PharmaProject.Models.Job>

@{
    ViewBag.Title = "joblist";
}

<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">



<table class="table table-bordered">
    <tr>
        <th style="font-size:x-large">
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th style="font-size:x-large">
            @Html.DisplayNameFor(model => model.Father_Name)
        </th>
        <th style="font-size:x-large">
            @Html.DisplayNameFor(model => model.Email_Address)
        </th>
        <th style="font-size:x-large">
            @Html.DisplayNameFor(model => model.Phone_number)
        </th>
         <th style="font-size:x-large">
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th style="font-size:x-large">
            @Html.DisplayNameFor(model => model.State)
        </th>
        <th style="font-size:x-large">
            @Html.DisplayNameFor(model => model.DOB)
        </th>
        <th style="font-size:x-large">
            @Html.DisplayNameFor(model => model.Qualification_)
        </th>
        <th style="font-size:x-large">
            @Html.DisplayNameFor(model => model.Education)
        </th>
        <th style="font-size:x-large">
            @Html.DisplayNameFor(model => model.Resume)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td style="font-size:medium">
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td style="font-size:medium">
                @Html.DisplayFor(modelItem => item.Father_Name)
            </td>
            <td style="font-size:medium">
                <a href="https://mail.google.com/mail/u/0/#compose">@Html.DisplayFor(modelItem => item.Email_Address)</a>
            </td>
            <td style="font-size:medium">
                @Html.DisplayFor(modelItem => item.Phone_number)
            </td>
            <td style="font-size:medium">
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td style="font-size:medium">
                @Html.DisplayFor(modelItem => item.State)
            </td>
            <td style="font-size:medium">
                @Html.DisplayFor(modelItem => item.DOB)
            </td>
            <td style="font-size:medium">
                @Html.DisplayFor(modelItem => item.Qualification_)
            </td>
            <td style="font-size:medium">
                @Html.DisplayFor(modelItem => item.Education)
            </td>
            <td style="font-size:medium">
                @Html.DisplayFor(modelItem => item.Resume)
            </td>
            <td style="font-size:medium">
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
              
            </td>
        </tr>
    }

</table>
